//
// Loop Tests
//

print("1) Simple loop with switch. Should print 3, 2, 1\n");

var arr = [ "abc", 5, 3.5, 4 ];
for (var i = 0; i < arr.length; ++i) {
	switch (arr[i]) {
		case 4:
		case 3.5: print("1\n");
		default: print("2\n");
		case "abc": print("3\n");
	}
	if (i == 1) {
		continue;
	}
	if (i == 2) {
		break;
	}
}

print("\n2) while with break and continue, should print 1, 3, 4, 5, 6\n");

var i = 1;
while (i < 10) {
	if (i == 2) {
		i++;
		continue;
	}
	println(i);
	if (i > 5) {
		break;
	}
	i++;
}

print("\n3) do..while, should print 20 - 11\n");
var i = 20;
do {
	println(i);
	i--;
} while(i > 10);
