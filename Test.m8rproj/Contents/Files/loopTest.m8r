//
// Loop Tests
//

var arr = [ "abc", 5, 3.5, 4 ];
for (var i = 0; i < arr.length; ++i) {
	switch (arr[i]) {
		case 4:
		case 3.5: Serial.print("Got 3.5 or 4\n");
		default: Serial.print("Got default\n");
		case "abc": Serial.print("Got abc\n");
	}
	if (i == 1) {
		continue;
	}
	Serial.print("Before break\n");
	if (i == 2) {
		break;
	}
}

Serial.print("Test while, should print 1 - 9\n");

var i = 1;
while (i < 10) {
	if (i == 2) {
		i++;
		continue;
	}
	Serial.print("i=" + i + "\n");
	if (i > 5) {
		break;
	}
	i++;
}

Serial.print("\nTest do..while, should print 20 - 11\n");
var i = 20;
do {
	Serial.print("i=" + i + "\n");
	i--;
} while(i > 10);
