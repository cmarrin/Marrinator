//
// Loop Tests
//

var arr = [ "abc", 5, 3.5 ];
for (var i = 0; i < 3; ++i) {
	switch (arr[i]) {
		case 3.5: Serial.print("Got 3.5\n");
		default: Serial.print("Got default\n");
		case "abc": Serial.print("Got abc\n");
	}
}

