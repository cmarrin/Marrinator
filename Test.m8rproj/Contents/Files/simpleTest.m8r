function add()
{
	var n = 0;
	for (var i = 0; i < arguments.length; ++i) {
		n += arguments[i];
	}
	return n;
}

print("add() = " + add() + "\n");
print("add(8) = " + add(8) + "\n");
print("add(5, 7) = " + add(5, 7) + "\n");
print("add(4, 3, 2) = " + add(4, 3, 2) + "\n");
print("add(1, 2, 3, 4) = " + add(1, 2, 3, 4) + "\n");

