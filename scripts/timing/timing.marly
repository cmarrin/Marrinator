//
// Timing test
//

[ ] @a
4000 @n
$n $a:length

"\n\nTiming test: " $n " squared iterations\n" cat cat print

currentTime @startTime

0 [dup $n lt] [inc] [
    0 [dup $n lt] [inc] [
        3 @f
        
        // a[j] = j * (j + 1) / 2;
        dup $a swap dup dup 1 + * 2 / swap insert
    ] for
] for

"Run time: " currentTime $startTime - "ms\n\n" cat cat print
