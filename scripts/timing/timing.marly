//
// Timing test
//

[ ] @a
4000 @n
$n $a:length

"\n\nTiming test: " $n " squared iterations\n" cat cat print

currentTime dup @startTime "*** start=" swap cat " seconds\n" cat print

0 [dup $n lt] [inc] [
    0 [dup $n lt] [inc] [
        3 @f
        
        // a[j] = j * (j + 1) / 2;
        dup $a swap dup dup 1 + * 2 / swap atput
    ] for
] for

currentTime dup "*** end=" swap cat " seconds\n" cat print
$startTime -
"Run time: " swap cat " seconds\n\n" cat print
